<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="handong.cconma.cconmaadmin" >

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BoardWrite"/>
        <activity
            android:name=".BoardModify"/>            
        <activity android:name=".BoardCconma"></activity>
        <activity android:name=".BoardView"
            android:windowSoftInputMode="adjustResize"></activity>
        <activity android:name=".BoardMarked"></activity>

        <activity android:name=".Inquiry.InquryMember"/>
        <activity android:name=".Inquiry.InquryOrder"/>
        <activity android:name=".OneToOne"/>
        <activity android:name=".Push.PushView"/>
        <service
            android:name=".IconService">
            <intent-filter>
                <action android:name="iconservice"/>
            </intent-filter>
        </service>
    </application>


</manifest>
