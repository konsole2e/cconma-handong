<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="handong.cconma.cconmaadmin">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Statics.StaticsMain"></activity>
        <activity
            android:name=".Statics.StaticsOrderH"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name=".Statics.StaticsLike"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name=".Statics.StaticsOrderRecent"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name=".Statics.StaticsTrade"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name=".Statics.StaticsMember"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name=".Statics.StaticsMemberRecent"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity android:name=".Board.BoardWrite" />
        <activity android:name=".Board.BoardModify" />
        <activity android:name=".Board.BoardCconma"></activity>
        <activity android:name=".Board.BoardView"></activity>
        <activity android:name=".Board.BoardMarked"></activity>

        <service android:name=".IconService">
            <intent-filter>
                <action android:name="iconservice" />
            </intent-filter>
        </service>
    </application>

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

</manifest>
