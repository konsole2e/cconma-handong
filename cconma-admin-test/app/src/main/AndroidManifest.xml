<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cconma.cconma_test_actionbar" >

    <uses-permission android:name="android.permission.INTERNET" />

    <!-- uses-permissions for GCM -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />

    <permission android:name="com.example.gcm.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />
    <uses-permission android:name="com.example.gcm.permission.C2D_MESSAGE" />
    <!-- -->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name="com.cconma.cconma_admin_test$Activity.MainActivity"
            android:label="@string/company_name" >
            <intent-filter>
                <action android:name="com.cconma.cconma_test_actionbar.MainActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.cconma.cconma_admin_test$Activity.LoginWebView">
        </activity>

        <activity
            android:name="com.cconma.cconma_admin_test$Activity.BoardWriteActivity">
        </activity>


        <!-- THESE ARE FOR GCM -->
        <receiver
            android:name="com.google.android.gms.gcm.GcmReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <category android:name="net.saltfactory.demo.gcm" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.cconma.cconma_admin_test$gcm.MyGcmListenerService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            </intent-filter>
        </service>

        <service
            android:name="com.cconma.cconma_admin_test$gcm.MyInstanceIDListenerService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.android.gms.iid.InstanceID"/>
            </intent-filter>
        </service>

        <service
            android:name="com.cconma.cconma_admin_test$gcm.RegistrationIntentService"
            android:exported="false">
        </service>
        <!-- END OF GCM -->

    </application>

</manifest>
