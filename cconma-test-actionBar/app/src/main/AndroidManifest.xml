<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cconma.cconma_test_actionbar" >

    <uses-permission android:name="android.permission.INTERNET" />

    <!-- uses-permissions for GCM -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />

    <permission android:name="com.cconma.cconma_test_actionbar.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />
    <uses-permission android:name="com.cconma.cconma_test_actionbar.permission.C2D_MESSAGE" />
    <!-- -->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".Activity.MainActivity"
            android:label="@string/company_name" >
            <intent-filter>
                <action android:name="com.cconma.cconma_test_actionbar.MainActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".view.LoginWebView"
            android:label="@string/company_name"
            android:parentActivityName=".Activity.MainActivity">
            <intent-filter>
                <action android:name="arabiannight.tistory.com.intentfilter.LoginWebView">
                    <category android:name="android.intent.category.DEFAULT">
                    </category></action></intent-filter>
        </activity>

        <receiver
            android:name=".GcmBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <category android:name="com.example.gcm" />
            </intent-filter>
        </receiver>
        <service android:name=".gcm.GcmIntentService" />

    </application>

</manifest>
